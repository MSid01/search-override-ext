{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0",
  "description": "Customize Chrome settings with this extension.",
  "permissions": ["search"],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Yahoo",
      "keyword": "yahoo",
      "search_url": "https://search.yahoo.com/search?q={searchTerms}",
      "favicon_url": "https://s.yimg.com/rz/l/favicon.ico",
      "is_default": true,
      "encoding": "UTF-8"
    }
  }
}
